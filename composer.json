{
    "name": "2lenet/pdf-generator-bundle",
    "type": "symfony-bundle",
    "description": "Pdf generator",
    "keywords": ["pdf"],
    "license": "MIT",
    "authors": [
        {
            "name": "Saenger joris",
            "email": "joris@2le.net"
        },
        {
            "name": "Castro Guzman Matias",
            "email": "matias@2le.net"
        }
    ],
    "require": {
        "php": ">=7.2 || ^8.0",
        "laminas/laminas-escaper": "^2.0",
        "symfony/property-access": "^4.1 || ^5.0 || ^6.0",
        "phpoffice/phpword": "^0.17 || ^0.18",
        "laminas/laminas-stdlib": "^2.0 || ^3.0",
        "dompdf/dompdf": "^0.8",
        "tecnickcom/tcpdf": "^6.0",
        "vich/uploader-bundle": "^1.8",
    },
    "autoload": {
        "psr-4": { "Lle\\PdfGeneratorBundle\\": "src/" }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpstan/phpstan": "1.9.x-dev",
        "phpstan/phpstan-doctrine": "1.3.x-dev",
        "phpstan/phpstan-mockery": "1.1.x-dev",
        "phpstan/phpstan-phpunit": "^1.2",
        "phpstan/phpstan-symfony": "1.2.x-dev"
    }
}
