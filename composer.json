{
    "name": "2lenet/pdf-generator-bundle",
    "type": "symfony-bundle",
    "description": "Pdf generator",
    "keywords": ["pdf"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laminas/laminas-escaper": "^2.11",
        "symfony/property-access": "^6.0",
        "phpoffice/phpword": "^0.18",
        "tecnickcom/tcpdf": "^6.3",
        "vich/uploader-bundle": "^1.19 || ^2.0",
        "symfony/yaml": "^6.0",
        "laminas/laminas-stdlib": "^3.15",
        "symfony/http-foundation": "^6.0",
        "doctrine/orm": "^2.6 || ^3.0",
        "symfony/routing": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "doctrine/annotations": "^1.13",
        "symfony/serializer": "^6.0",
        "twig/twig": "^3.4.3",
        "setasign/fpdi": "^2.3.6",
        "symfony/process": "^6.0",
        "setasign/fpdf": "1.8.4",
        "alexanderpavlov/pdfmerger": "^1.0",
        "symfony/dotenv": "^6.0"
    },
    "autoload": {
        "psr-4": { "Lle\\PdfGeneratorBundle\\": "src/" }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1"
    }
}
